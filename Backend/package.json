{
   "name":"holdkrykke",
   "version":"1.0.0",
   "description":"",
   "scripts":{
      "dev":"node -r ts-node/register ./src/app.ts",
      "dev:watch":"nodemon",
      "start":"tsc && node ./build/app.js",
      "build":"tsc"
   },
   "nodemonConfig":{
      "ignore":[
         "test/**/*",
         ".git",
         "node_modules"
      ],
      "watch":[
         "src"
      ],
      "exec":"npm run dev",
      "ext":"ts"
   },
   "dependencies":{
      "express":"^4.17.1",
      "mongodb":"^4.2.0",
      "typescript":"^4.5.2"
   },
   "devDependencies":{
      "ts-node":"^10.4.0"
   }
}