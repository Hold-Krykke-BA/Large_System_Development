{
  "name": "holdkrykke",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "node -r ts-node/register ./src/app.ts",
    "dev:watch": "nodemon",
    "start": "tsc && node ./build/app.js",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "test/**/*",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "dependencies": {
    "@types/mongodb": "^4.0.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ip-range-check": "^0.2.0",
    "mongodb": "^4.2.0",
    "request-ip": "^2.1.3",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "ts-node": "^10.4.0"
  }
}
